#
#. type: TP
#: raw/coreutils/man1/shred.1:35
#, no-wrap
msgid "B<-v>, B<--verbose>"
msgstr "B<-v>, B<--verbose>"

#
#. type: Plain text
#: raw/coreutils/man1/shred.1:38
msgid "show progress"
msgstr "显示进度"

#
#. type: TP
